{
  "bracketSpacing": true,
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "es5",
  "printWidth": 120,
  "overrides": [
    {
      "files": "package.json",
      "options": {
        "tabWidth": 2,
        "useTabs": false,
        "printWidth": 512
      }
    },
    {
      "files": "*.md",
      "options": {
        "tabWidth": 4,
        "useTabs": false
      }
    }
  ]
}
